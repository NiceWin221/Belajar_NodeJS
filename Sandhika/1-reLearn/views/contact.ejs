<div class="container mt-3">
  <div class="row">
    <div class="cold-md-8">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Nama</th>
            <th scope="col">Email</th>
            <th scope="col">Detail</th>
          </tr>
        </thead>
        <tbody>
          <% mahasiswa.forEach((mhs, index) => { %>
          <tr>
            <th scope="row"><%= index + 1 %></th>
            <td><%= mhs.nama %></td>
            <td><%= mhs.email %></td>
            <td>
              <button class="btn btn-success badge">Detail</button>
            </td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>
